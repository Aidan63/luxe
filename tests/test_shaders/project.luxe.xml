<?xml version="1.0" encoding="utf-8"?>
<project>
	
	<meta 
          title="luxe - shaders" 
          package="com.luxeengine.luxetestshaders" 
          version="1.0.0" 
          company="luxeengine" 
	/>

	<app main="Main" path="bin" file="luxe_test_shaders" />

	<window width="960" height="640" orientation="landscape" fps="60" vsync="true" resizable="true" />
	
     <assets path="Assets" rename="assets" include="*"/>

	<source path="src" />
	
	<haxelib name="luxe" />
	<haxelib name="lime" />
	

