<launchImage path="iOS/Default-Landscape~ipad.png" width="1024" height="768" if="ios"/>
<launchImage path="iOS/Default-Landscape@2x~ipad.png" width="2048" height="1536" if="ios"/>	

<icon width="57" height="57" path="iOS/icon57.png"/>
